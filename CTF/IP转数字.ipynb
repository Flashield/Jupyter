    "import socket\n",
    "import struct\n",
    "ip = '127.0.0.1'\n",
    "Long=socket.ntohl(struct.unpack(\"I\",socket.inet_aton(str(ip)))[0])\n",
    "print(Long)\n",
    "print(socket.inet_ntoa(struct.pack('I',socket.htonl(Long))))"
