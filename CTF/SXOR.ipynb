   "source": [
    "import string\n",
    "import itertools\n",
    "def sxor(s1, s2):\n",
    "    return ''.join(\n",
    "        chr(ord(a) ^ ord(b))\n",
    "        for a, b in zip(s1, itertools.cycle(s2))\n",
    "    )"
   ]
