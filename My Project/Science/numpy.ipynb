   "source": [
    "#list转array\n",
    "x = [[1.2,2.2,1.4,3],[1.3,2.4,2.1,4],[1,1,0,5]]\n",
    "arr = np.array(x)\n",
    "print(arr)\n",
    "#array转置\n",
    "print(arr.T)\n",
    "m = np.matrix(x)\n",
    "print(m)\n",
    "print(m.T)\n",
    "print(type(arr),type(m))\n",
    "print(m.tolist)"
   ]
